<div class="page-header">  <div>    <h1>Admin Dashboard</h1>    <p>Oversee elections, manage voters, and monitor results effortlessly.</p>  </div></div><!-- Stats Overview --><div class="grid grid-cols-1 gap-5 sm:grid-cols-2 lg:grid-cols-3 mb-8">  <% Admin::SidebarHelper::MENU_LISTS[:student].each do |list| %>    <div class="bg-white overflow-hidden shadow rounded-lg">      <div class="p-5">        <div class="flex items-center">          <div class="flex-shrink-0">            <%= image_tag "admin/sidebar-icons/#{list}-dark.svg", class: 'h-6 w-6 text-gray-400' %>          </div>          <div class="ml-5 w-0 flex-1">            <dl>              <dt class="text-sm font-medium text-gray-500 truncate">Total <%= list.titleize %></dt>              <dd class="flex items-baseline">                <div class="text-2xl font-semibold text-gray-900"><%= total_count(list) %></div>              </dd>            </dl>          </div>        </div>      </div>    </div>  <% end %></div>